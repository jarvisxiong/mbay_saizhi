<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<!-- 获取相关信息Servlet（通用） -->
	<servlet>
		<servlet-name>CommonGetServlet</servlet-name>
		<servlet-class>org.sz.mbay.redis.servlet.CommonGetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CommonGetServlet</servlet-name>
		<url-pattern>/common/get</url-pattern>
	</servlet-mapping>

	<!-- 存入相关信息Servlet（通用） -->
	<servlet>
		<servlet-name>CommonSetServlet</servlet-name>
		<servlet-class>org.sz.mbay.redis.servlet.CommonSetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CommonSetServlet</servlet-name>
		<url-pattern>/common/set</url-pattern>
	</servlet-mapping>
	
	<!-- 删除相关信息Servlet（通用） -->
	<servlet>
		<servlet-name>CommonDelServlet</servlet-name>
		<servlet-class>org.sz.mbay.redis.servlet.CommonDelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CommonDelServlet</servlet-name>
		<url-pattern>/common/del</url-pattern>
	</servlet-mapping>
</web-app>
